package com.midai.miya.user.model;

import java.util.Date;
import java.io.Serializable;
public class Reward implements Serializable {

    private static final long serialVersionUID = 1L;
    /**
     * ID
     */
    private String rewardId;
    /**
     * 打赏人ID
     */
    private String rewardUserId;
    /**
     * 打赏人姓名
     */
    private String rewardUserName; 
    /**
     * 专辑ID
     */
    private String rewardInterestId;
    /**
     * 专辑名称
     */
    private String interestTitle;
    /**
     * 被打赏人
     */
    private String rewardToUserId;
    /**
     * 被打赏人姓名
     */
    private String rewardToUserName;
    /**
     * 礼品ID
     */
    private String rewardGiftId;
    /**
     * 打赏数量
     */
    private Integer rewardGiftNum;
    /**
     * 打赏金额
     */
    private Double rewardPrice;
    /**
     * 1 帐户余额，2 支付宝 3微信支付
     */
    private Integer rewardPayeeType;
    /**
     * 打赏时间
     */
    private Date rewardCreateTime;
    
    private String queryBeginRegisterTime;
    private String queryEndRegisterTime;
    public String getRewardId(){
        return rewardId;
    }
    public void setRewardId(String rewardId){
        this.rewardId=rewardId;
    }
    public String getRewardUserId(){
        return rewardUserId;
    }
    public void setRewardUserId(String rewardUserId){
        this.rewardUserId=rewardUserId;
    }
    public String getRewardInterestId(){
        return rewardInterestId;
    }
    public void setRewardInterestId(String rewardInterestId){
        this.rewardInterestId=rewardInterestId;
    }
    public String getRewardToUserId(){
        return rewardToUserId;
    }
    public void setRewardToUserId(String rewardToUserId){
        this.rewardToUserId=rewardToUserId;
    }
    public String getRewardGiftId(){
        return rewardGiftId;
    }
    public void setRewardGiftId(String rewardGiftId){
        this.rewardGiftId=rewardGiftId;
    }
    public Integer getRewardGiftNum(){
        return rewardGiftNum;
    }
    public void setRewardGiftNum(Integer rewardGiftNum){
        this.rewardGiftNum=rewardGiftNum;
    }
    public Integer getRewardPayeeType(){
        return rewardPayeeType;
    }
    public void setRewardPayeeType(Integer rewardPayeeType){
        this.rewardPayeeType=rewardPayeeType;
    }
    public Date getRewardCreateTime(){
        return rewardCreateTime;
    }
    public void setRewardCreateTime(Date rewardCreateTime){
        this.rewardCreateTime=rewardCreateTime;
    }
	public Double getRewardPrice() {
		return rewardPrice;
	}
	public void setRewardPrice(Double rewardPrice) {
		this.rewardPrice = rewardPrice;
	}
	public String getRewardUserName() {
		return rewardUserName;
	}
	public void setRewardUserName(String rewardUserName) {
		this.rewardUserName = rewardUserName;
	}
	public String getRewardToUserName() {
		return rewardToUserName;
	}
	public void setRewardToUserName(String rewardToUserName) {
		this.rewardToUserName = rewardToUserName;
	}
	public String getInterestTitle() {
		return interestTitle;
	}
	public void setInterestTitle(String interestTitle) {
		this.interestTitle = interestTitle;
	}
	public String getQueryBeginRegisterTime() {
		return queryBeginRegisterTime;
	}
	public void setQueryBeginRegisterTime(String queryBeginRegisterTime) {
		this.queryBeginRegisterTime = queryBeginRegisterTime;
	}
	public String getQueryEndRegisterTime() {
		return queryEndRegisterTime;
	}
	public void setQueryEndRegisterTime(String queryEndRegisterTime) {
		this.queryEndRegisterTime = queryEndRegisterTime;
	}
}